{
    "all": true,
    "branches": 100,
    "include": [
        "crackers/**/*.js",
        "devops/**/*.js",
        "emulators/**/*.js",
        "src/**/*.js",
        "tools/**/*.js"
    ],
    "exclude": [
        "test/*.js",
        "src/types.js"
    ],
    "excludeNodeModules": true,
    "functions": 100,
    "lines": 100,
    "reportDir": ".coverage",
    "statements": 100,
    "reporter": [
        "html",
        "text"
    ]
}